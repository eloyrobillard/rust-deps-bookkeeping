{
  "name": "reaqta-maia-common",
  "version": "3.12.0",
  "private": true,
  "description": "utilities shared between maia frontend and middle-end",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "testci": "NODE_ENV=test jest --no-cache --all",
    "build": "npm run clean && tsc --removeComments -p tsconfig-build.json && tsc-alias -p tsconfig-build.json",
    "build:dev": "npm run clean && tsc -p tsconfig-build.json && tsc-alias -p tsconfig-build.json",
    "clean": "rm -rf ./ts/*",
    "console": "ts-node -r tsconfig-paths/register",
    "docs": "typedoc --entryPointStrategy expand --excludeInternal --exclude '**/*example.ts' --sort source-order --customCss ./typedoc.css src",
    "ts:check": "tsc --noEmit -p tsconfig-build.json"
  },
  "dependencies": {
    "html-entities": "^1.3.1",
    "lodash.find": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.map": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "lodash.uniq": "^4.5.0",
    "password-validator": "^4.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^17.0.36",
    "core-js": "3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.8.0",
    "tsc-alias": "^1.6.9",
    "tsconfig-paths": "^4.0.0",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.2"
  }
}
