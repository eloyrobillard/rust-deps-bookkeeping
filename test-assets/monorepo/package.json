{
  "name": "maia",
  "version": "3.12.0",
  "description": "Monolith javascript project that holds both the backend and frontend for Hive's dashboard.",
  "private": true,
  "workspaces": [
    "common/",
    "frontend/",
    "backend/",
    "tools/utils/"
  ],
  "scripts": {
    "start:pgadmin": "tools/scripts/start-pgadmin.sh",
    "start:postgres": "tools/scripts/start-postgres.sh",
    "start:rabbit": "tools/scripts/start-rabbit.sh",
    "start:dev": "tools/scripts/dev.sh",
    "start:staging": "tools/scripts/start-maia.sh",
