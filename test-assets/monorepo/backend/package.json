{
  "name": "reaqta-maia-backend",
  "version": "3.12.0",
  "private": true,
  "scripts": {
    "build": "npm run build:clear && babel app --extensions \".js,.ts\" -d dist",
    "build:clear": "rm -rf dist",
    "serve:dev": "NODE_ENV=development babel-node --watch --extensions \".js,.ts\" ./bin/start-dev.mjs",
    "serve:dev:cp4s": "MAIA_CP4S_SECRETS=$PWD/cp4s npm run serve:dev",
    "serve": "NODE_ENV=production node ./bin/start-prod.mjs",
    "start:dev": "nodemon --exec npm run serve:dev",
    "start:dev:cp4s": "nodemon --exec npm run serve:dev:cp4s",
    "dev": "npm run build && npm run db:groupSync && npm run start:dev",
    "repl": "node dist/app-repl",
    "test": "NODE_ENV=test jest --detectOpenHandles --logHeapUsage",
    "test:coverage": "npm run test --collectCoverage",
    "test:db:migrate": "NODE_ENV=test npm run db:migrate",
    "test:groupSync": "NODE_ENV=test jest --detectOpenHandles -c='tests/jest-configs/groupSync.json'",
    "test:unit": "NODE_ENV=test SKIP_JEST_GLOBAL_SETUP=true jest unit --detectOpenHandles --logHeapUsage",
    "test:int": "NODE_ENV=test jest int --detectOpenHandles --logHeapUsage",
    "test:custom": "NODE_ENV=test jest --detectOpenHandles --logHeapUsage",
    "test:custom:unit": "NODE_ENV=test SKIP_JEST_GLOBAL_SETUP=true jest --detectOpenHandles --logHeapUsage",
    "testci": "npm run testci:groupSync && NODE_ENV=test jest --forceExit --no-cache --detectOpenHandles",
    "testci:groupSync": "npm run test:groupSync --forceExit --no-cache",
    "db:migrate:create": "node create-migration",
    "db:migrate": "sequelize db:migrate",
    "db:migrateci": "NODE_ENV=test sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:setup": "node dist/setup.js",
    "db:groupSync": "npm run repl 'process.env.SKIP_GROUP_SYNC && this.exit; this.Group.syncWithHive().then(() => { console.log(\"Group synchronized\\!\"); this.exit; }).catch(err => { console.error(err); process.exit(1) })'",
    "feature:setDevelDefaults": "node dist/featureFlagSyncDefaults",
    "feature:on": "node dist/featureFlagCrud --on -n",
    "feature:off": "node dist/featureFlagCrud --off -n",
    "feature:list": "node dist/featureFlagCrud --list -n",
    "scripts": "cat package.json | jq '.scripts'",
    "2fatoken": "node dist/generate-2fa-token.js",
    "silence": "npm run repl 'this.turnOffEmails().then(() => { console.log(\"Ahhhhhhh yes. The sound of silence.\"); this.exit; })'",
    "ts:check": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.5.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^16",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "core-js": "3",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "nodemon": "^2.0.15",
    "sequelize-mock": "^0.10.2",
    "supertest": "^6.3.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@reaqta/express-brute": "^1.0.2",
    "axios": "0.24.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.0",
    "connect-flash": "^0.1.1",
    "connect-session-sequelize": "^7.0.0",
    "cookie-parser": "^1.4.0",
    "cors": "2.8.5",
    "csurf": "^1.9.0",
    "dotenv": "^4.0.0",
    "ejs": "^3.1.7",
    "email-templates": "^11.0.0",
    "express": "^4.18.2",
    "express-session": "^1.14.2",
    "express-socket.io-session": "^1.3.1",
    "foo-foo-mq": "^7.0.0",
    "helmet": "^3.13.0",
    "hunq": "../shame-deps/hunq-0.13.3.tgz",
    "jose": "^4.13.1",
    "lodash": "^4.17.21",
    "long": "^4.0.0",
    "lua-fmt": "^2.6.0",
    "luaparse": "0.2.1",
    "minimist": "^1.2.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.41",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.6.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-remember-me": "0.0.1",
    "passport.socketio": "^3.7.0",
    "pg": "^8.7.1",
    "pluralize": "^7.0.0",
    "puppeteer": "13.5.0",
    "qrcode": "^1.2.2",
    "semver": "^7.3.2",
    "sequelize": "^6.29.3",
    "sequelize-cli": "^6.6.0",
    "socket.io": "^4.5.4",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^4.6.2",
    "ua-parser-js": "^0.7.33",
    "uuid": "^8.3.2"
  }
}
